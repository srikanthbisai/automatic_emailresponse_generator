      "@types/jest": "^29.0.0",
      "@typescript-eslint/eslint-plugin": "^6.0.0",
      "@typescript-eslint/parser": "^6.0.0",
      "eslint": "^8.0.0",
      "jest": "^29.0.0",
      "rimraf": "^5.0.0",
      "ts-jest": "^29.0.0",
      "ts-node-dev": "^2.0.0",
